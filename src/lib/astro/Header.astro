---
import { Navbar } from '@daks.dev/astro.sdk/astro/navbar';

import { app, nav } from '$app/configs';
const { name: label } = app;
const {
  navbar: { items }
} = nav;
---

<Navbar
  {items}
  custom={{
    home: 'h-8',
    //theme: 'ml-8',
    item: 'h-10 bp:h-full'
  }}
  brand={{
    icon: {
      home: 'code-block',
      page: 'code'
    },
    label
  }}
/>
